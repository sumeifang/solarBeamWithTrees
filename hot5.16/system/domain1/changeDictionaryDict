FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      changeDictionaryDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

boundary
{
    window_inlet
    {
        type            mappedWall;
        sampleMode      nearestPatchFace;
        sampleRegion    atrium;
        samplePatch    window_inlet;

    }
	    window_outlet
    {
        type           mappedWall;
        sampleMode      nearestPatchFace;
        sampleRegion    atrium;
        samplePatch    window_outlet;

    }
	    building_inlet
    {
        type            mappedWall;
        sampleMode      nearestPatchFace;
        sampleRegion    building;
        samplePatch    building_inlet;

    }
	    building_outlet
    {
        type           mappedWall;
        sampleMode      nearestPatchFace;
        sampleRegion    building;
        samplePatch    building_outlet;

    }
	glass
    {
        type            mappedWall;
        sampleMode      nearestPatchFace;
        sampleRegion    atrium;
        samplePatch    glass;
    }
}


