FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{

    p_rgh
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-06;
        relTol          0.01;
    }

    "(U|h|k|epsilon|R)"
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-05;
        relTol          0.1;
    }
	Ii
    {
        solver              GAMG;
        tolerance           1e-4;
        relTol              0.1;
        smoother            symGaussSeidel;
        maxIter             10;
        nPostSweeps         1;
    }
}

SIMPLE
{
    momentumPredictor yes;
    nNonOrthogonalCorrectors 1;
    pRefCell        0;
    pRefValue       100000;
    rhoMin          0.2;
    rhoMax          2;
}


relaxationFactors
{
    fields
    {
        p_rgh           0.7;
		rho             1.0;
    }
    equations
    {
        U               0.3;
        h               0.5;
		G               0.7;
        "(k|epsilon|R)" 0.7;
		T               0.9;
        eT              0.6;
        kT              0.6;
		"ILambda.*"     0.7;
		qr				0.3;
    }
}


