FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/solid";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 293;

boundaryField
{
	/*window_inlet
    {
        type            fixedValue;
        value           $internalField;
    }*/
	window_outlet
	{
        type            zeroGradient;
    }
	
		window_inlet
    {
		type            mappedField;
		sampleMode      nearestPatchFace;
		samplePatch 	window_inlet;
		sampleRegion    domain1;
		//offsetMode 		uniform;
		//offset  		(0 0 0);
		//interpolationScheme  cell;
        field           T;              // optional field name
        setAverage      no;             // apply an average value
        average         0;              // average to apply if setAverage
        value           uniform  293;		// place holder
		//inletValue      uniform  0;
		//Prt             0.85; 
    }
	

   glass
    {
        type            compressible::turbulentTemperatureRadCoupledMixed;
        value           uniform 293;
        Tnbr            T;
        kappaMethod     fluidThermo;
        qrNbr           qr;
        qr              qr;
        kappa           none;
		thicknessLayers (0.01);//list of thicknesses per layer [m]
		kappaLayers     (1);//list of thermal conductivites per layer [W/m/K]
		thermalInertia  true;
    }
		atrium_to_domain1
    {
        type            compressible::turbulentTemperatureRadCoupledMixed;
        value           uniform 293;
        Tnbr            T;
        kappaMethod     fluidThermo;
        qrNbr           qr;
        qr              qr;
        kappa           none;
		thicknessLayers (0.2);//list of thicknesses per layer [m]
		kappaLayers     (0.5);//list of thermal conductivites per layer [W/m/K]
		thermalInertia  true;
    }
	atrium_to_building
    {
        type            compressible::turbulentTemperatureRadCoupledMixed;
        value           uniform 293;
        Tnbr            T;
        kappaMethod     fluidThermo;
        qrNbr           qr;
        qr              qr;
        kappa           none;
		thicknessLayers (0.2);//list of thicknesses per layer [m]
		kappaLayers     (0.5);//list of thermal conductivites per layer [W/m/K]
		thermalInertia  true;
    }
	atrium_to_floor
    {
        type            compressible::turbulentTemperatureRadCoupledMixed;
        value           uniform 293;
        Tnbr            T;
        kappaMethod     fluidThermo;
        qrNbr           none;
        qr              qr;
        kappa           none;
    }
}


