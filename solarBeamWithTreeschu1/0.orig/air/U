FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0/air";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 -1 0 0 0 0 ];

internalField   uniform ( 0.5 0 0 );

boundaryField
{
    maxY
    {
        type            pressureInletOutletVelocity;
        value           $internalField;
        phi             phi;
    }
    minX
    {
        type            pressureInletOutletVelocity;
        value           $internalField;
        phi             phi;
    }
    maxX
    {
        type            pressureInletOutletVelocity;
        value           $internalField;
        phi             phi;
    }
    minY
    {
        type            pressureInletOutletVelocity;
        value           $internalField;
        phi             phi;
    }
    maxZ
    {
        type            pressureInletOutletVelocity;
        value           $internalField;
        phi             phi;
    }
    air_to_floor
    {
        type            fixedValue;
        value           uniform ( 0 0 0 );
        phi             phi;
    }
    "(air_to_solid|inlet|outlet)"
    {
        type            fixedValue;
        value           uniform ( 0 0 0 );
        phi             phi;
    }
	    glass
    {
        type            fixedValue;
        value           uniform ( 0 0 0 );
        phi             phi;
    }
}


