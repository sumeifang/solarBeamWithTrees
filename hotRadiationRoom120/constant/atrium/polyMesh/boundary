FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       polyBoundaryMesh;
    location    "constant/atrium/polyMesh";
    object      boundary;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


4
(
    window
    {
        type            mappedWall;
        nFaces          52;
        startFace       66554;
        inGroups        2 ( wall mappedPatch );
        sampleMode      nearestPatchFace;
        sampleRegion    atrium;
        samplePatch     atrium_to_domain1;
    }

    atrium_to_floor
    {
        type            mappedWall;
        inGroups        2 ( wall mappedPatch );
        nFaces          877;
        startFace       66606;
        sampleMode      nearestPatchFace;
        sampleRegion    floor;
        samplePatch     floor_to_atrium;
    }

    atrium_to_building
    {
        type            mappedWall;
        inGroups        2 ( wall mappedPatch );
        nFaces          5394;
        startFace       67483;
        sampleMode      nearestPatchFace;
        sampleRegion    building;
        samplePatch     building_to_atrium;
    }

    atrium_to_domain1
    {
        type            mappedWall;
        inGroups        2 ( wall mappedPatch );
        nFaces          2189;
        startFace       72877;
        sampleMode      nearestPatchFace;
        sampleRegion    domain1;
        samplePatch     domain1_to_atrium;
    }

)


