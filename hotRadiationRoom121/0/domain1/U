FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0/air";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 -1 0 0 0 0 ];

internalField   uniform ( 0 0 0 );

boundaryField
{
   ".*"
    {
        type            pressureInletOutletVelocity;
        value           $internalField;
        phi             phi;
    }
		window
    {
        type            fixedValue;
        value           uniform ( 0 0 0 );
		phi             phi;
    }
	 inlet
    {
        type            fixedValue;
        value           uniform ( 2 0 0 );
		        phi             phi;
    }
     domain1_to_building
    {
        type            fixedValue;
        value           uniform ( 0 0 0 );
        phi             phi;
    }
	  domain1_to_atrium
    {
        type            fixedValue;
        value           uniform ( 0 0 0 );
        phi             phi;
    }
	  domain1_to_floor
    {
        type            fixedValue;
        value           uniform ( 0 0 0 );
        phi             phi;
    }
}


