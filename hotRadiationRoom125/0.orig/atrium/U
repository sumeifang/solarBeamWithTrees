FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0/air";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 -1 0 0 0 0 ];

internalField   uniform ( 0.1 0 0 );

boundaryField
{
    glass
    {
        type            noSlip;
    }
    atrium_to_building
    {
        type            noSlip;
    }
	    atrium_to_domain1
    {
        type            noSlip;
    }
	atrium_to_floor
    {
        type            noSlip;
    }

}


