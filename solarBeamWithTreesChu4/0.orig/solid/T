FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/solid";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 300;

boundaryField
{
    walls
    {
        type            compressible::turbulentTemperatureRadCoupledMixed;//wall：Coupled指流体还是固体
        value           uniform 300;
        Tnbr            T;
        kappaMethod     fluidThermo;
        qrNbr           qr;
        qr              qr;
        kappa           none;
    }
      glass
    {
        type            compressible::turbulentTemperatureRadCoupledMixed;//wall：Coupled指流体还是固体
        value           uniform 300;
        Tnbr            T;
        kappaMethod     fluidThermo;
        qrNbr           qr;
        qr              qr;
        kappa           none;
    }
    floor1
    {
        type            compressible::turbulentTemperatureRadCoupledMixed;//wall：Coupled指流体还是固体
        value           uniform 300;
        Tnbr            T;
        kappaMethod     fluidThermo;
        qrNbr           qr;
        qr              qr;
        kappa           none;
    }
    solid_to_air
    {
        type            compressible::turbulentTemperatureRadCoupledMixed;//wall：Coupled指流体还是固体
        value           uniform 300;
        Tnbr            T;
        kappaMethod     fluidThermo;
        qrNbr           qr;
        qr              qr;
        kappa           none;
    }
    solid_to_floor
    {
        type            compressible::turbulentTemperatureRadCoupledMixed;
        value           uniform 300;
        Tnbr            T;
        kappaMethod     fluidThermo;
        qrNbr           none;
        qr              qr;
        kappa           none;
    }
}


